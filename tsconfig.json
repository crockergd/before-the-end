{
    "compilerOptions": {
        "target": "es2016",
        "moduleResolution": "bundler",
        "sourceMap": true,
        "outDir": "./www",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "include": [
        "src/**/*.ts",
        "typings/*.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}